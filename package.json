    "update-cayo": "npm i cayo@latest -D"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-replace": "^5.0.2",
    "cayo": "^1.2.2",
    "chalk": "^5.2.0",
    "sass": "^1.56.1",
    "svelte-preprocess": "^4.10.7"
  }
}